{
  "name": "@videojs/svg-sprite-generator",
  "version": "1.0.0",
  "description": "Effortlessly customize Video.js player icons",
  "license": "MIT",
  "author": {
    "name": "amtins",
    "email": "amtins.dev@gmail.com",
    "url": "https://github.com/amtins"
  },
  "type": "module",
  "bin": {
    "vjs-svg-sprite": "./bin/cli.js"
  },
  "scripts": {
    "build": "node index.js",
    "build:material": "node bin/cli.js generate material-icons.json",
    "prestart": "npm run build",
    "start": "http-server -c-1 -o vjs-svg-sprite",
    "prestart:material": "npm run build:material",
    "start:material": "http-server -c-1 -o material-design-icons"
  },
  "keywords": [
    "svg",
    "sprite",
    "icons",
    "player",
    "video.js"
  ],
  "dependencies": {
    "svg-sprite": "^2.0.2",
    "svgo": "^3.0.3"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "material-design-icons": "^3.0.1"
  }
}
